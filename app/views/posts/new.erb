<h3>Post form</h3>

<%= form_tag posts_path do %><br>
  <label>Post title:</label><br>
  <%= text_tag "post[title]" %><br>
 
  <label>Post description:</label><br>
  <%=text_area_tag "post[description]" %><br>
 
  <input hidden_field_tag authenticity_token, form_authenticity_token>
<%= submit_tag "Submit Post" %>
<% end %>
<%= params.inspect %>